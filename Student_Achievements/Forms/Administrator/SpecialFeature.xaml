<Window x:Class="Student_Achievements.Forms.SpecialFeature"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Специальные возможности" Height="325" Width="370" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen" FontFamily="Courier New" FontSize="24" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="ButImport" Content="Импорт БД" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="337" Height="60" FontSize="24" FontFamily="Courier New" FontWeight="Bold" Click="ButImport_Click"/>
        <Button x:Name="ButExport" Content="Экспорт БД" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Width="337" Height="60" FontSize="24" FontFamily="Courier New" FontWeight="Bold" Click="ButExport_Click"/>
        <Button x:Name="ButMenu" Content="В меню" HorizontalAlignment="Left" Margin="10,220,0,0" VerticalAlignment="Top" Width="337" Height="60" FontSize="24" FontFamily="Courier New" FontWeight="Bold" Click="ButMenu_Click"/>
    </Grid>
</Window>
